{
    "minimum-stability": "stable",
    "name": "b3nl/laravel-rest-scaffolding",
    "description": "Generates basic routes and requests for REST APIs.",
    "keywords": ["framework", "laravel", "generator", "rest", "api"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1 - 5.2",
        "squizlabs/php_codesniffer": "2.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "b3nl\\RESTScaffolding\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
